    <div class="panel panel-primary">
      <div class="panel-heading">
        <h3 class="panel-title">Demo: Field Defaults</h3>
      </div>
      <div class="panel-body">
        <div id="form"><form id="deform" method="POST" enctype="multipart/form-data" accept-charset="utf-8" class="deform">

  <fieldset class="deformFormFieldset">

    

    <input type="hidden" name="_charset_">
    <input type="hidden" name="__formid__" value="deform">

    

    

    <div class="form-group  " title="Song name" id="item-deformField1">

  <label for="deformField1" class="control-label required" id="req-deformField1">
    Artist
  </label>

  
    <input type="text" name="artist" value="Grandaddy" id="deformField1" class=" form-control ">
  

  

  <p class="help-block">
    Song name
  </p>
</div>

    <div class="form-group  " title="" id="item-deformField2">

  <label for="deformField2" class="control-label required" id="req-deformField2">
    Album
  </label>

  
    <input type="text" name="album" value="Just Like the Fambly Cat" id="deformField2" class=" form-control ">
  

  

  
</div>

    <div class="form-group  " title="Song name" id="item-deformField3">

  <label for="deformField3" class="control-label required" id="req-deformField3">
    Song
  </label>

  
    <input type="text" name="song" value="" id="deformField3" class=" form-control ">
  

  

  <p class="help-block">
    Song name
  </p>
</div>


    <div class="form-group">
      
        <button id="deformsubmit" name="submit" type="submit" class="btn btn-primary " value="submit">
          
          Submit
        </button>
      
    </div>

  </fieldset>

  

</form>
</div>
      </div>
    </div>

    <div class="panel panel-info">
      <div class="panel-heading">
        <h3 class="panel-title">Captured submission</h3>
      </div>
