        <style type="text/css">
           .linenodiv pre {
             word-break: normal;
           }
        </style>

        <!-- CSS -->
        <link rel="stylesheet" href="${request.static_url('stickercode:static/bootstrap.min.css')}" type="text/css" media="screen" charset="utf-8">
        

        <!-- JavaScript -->
